@model System.Collections.Generic.List<smartHookah.Models.Place>

@{
    ViewBag.Title = "Managed places";
}

<div class="well">
    <h2>Managed places</h2>
    <ul>
        @foreach (var place in Model)
        {
            <li?>@place.Name <a href="@Url.Action("Manage",new {id = place.Id})" class="btn btn-default">Manage</a></li?>
        }
    </ul>
</div>